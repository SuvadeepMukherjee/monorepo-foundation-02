      "@/plugins/lambda": ["packages/plugins/lambda/src/index.ts"],
      "@/shared/aws": ["packages/shared/aws/src/index.ts"],
      "@/shared/graphql": ["packages/shared/graphql/src/index.ts"],
      "@/shared/graphql/types": [
        "packages/shared/graphql/src/schema/schema.types.ts"
      ],
      "@/shared/search/mapping": [
        "packages/shared/search/src/mapping/index.ts"
      ],
      "@/shared/variables": ["packages/shared/variables/src/index.ts"]
