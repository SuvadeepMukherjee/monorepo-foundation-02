      "@/lambdas/libs/dashboards": [
        "packages/lambdas/libs/dashboards/src/index.ts"
      ],
      "@/lambdas/libs/export": ["packages/lambdas/libs/export/src/index.ts"],
      "@/lambdas/libs/import": ["packages/lambdas/libs/import/src/index.ts"],
      "@/lambdas/libs/integrations": [
        "packages/lambdas/libs/integrations/src/index.ts"
      ],
      "@/lambdas/libs/public-api": [
        "packages/lambdas/libs/public-api/src/index.ts"
